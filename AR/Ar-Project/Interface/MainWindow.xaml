                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ListBox}">
                        <dx:DXBorder x:Name="Border" BorderBrush="#FF515151" BorderThickness="{TemplateBinding BorderThickness}" Background="#FF2E2E2E" EnableDPICorrection="True" Padding="1" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
