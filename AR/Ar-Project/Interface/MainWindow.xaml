                            <ScrollViewer x:Name="Content" Focusable="False" Padding="{TemplateBinding Padding}" IsHitTestVisible="True" Opacity="100" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden">
                                <ItemsPresenter/>
